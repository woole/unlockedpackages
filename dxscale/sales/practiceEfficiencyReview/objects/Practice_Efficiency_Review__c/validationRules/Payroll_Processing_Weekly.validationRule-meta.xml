<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payroll_Processing_Weekly</fullName>
    <active>true</active>
    <description>Percentage must be between 0 and 100%.</description>
    <errorConditionFormula>OR( Payroll_Processing_Weekly__c &gt; 1.0,
Payroll_Processing_Weekly__c  &lt; 0.0
)</errorConditionFormula>
    <errorDisplayField>Payroll_Processing_Weekly__c</errorDisplayField>
    <errorMessage>Percentage per Week must be between 0 and 100%.</errorMessage>
</ValidationRule>
