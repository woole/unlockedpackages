<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Feeds_Capacity</fullName>
    <active>true</active>
    <description>Percentage must be between 0 and 100%.</description>
    <errorConditionFormula>OR( Bank_Feeds_Capacity__c &gt; 1.0,
Bank_Feeds_Capacity__c  &lt; 0.0
)</errorConditionFormula>
    <errorDisplayField>Bank_Feeds_Capacity__c</errorDisplayField>
    <errorMessage>Percentage of Clients must be between 0 and 100%.</errorMessage>
</ValidationRule>
