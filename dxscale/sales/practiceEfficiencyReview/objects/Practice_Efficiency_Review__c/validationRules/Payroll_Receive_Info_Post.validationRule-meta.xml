<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payroll_Receive_Info_Post</fullName>
    <active>true</active>
    <description>Percentage of Post/Manual must be between 0 and 100%.</description>
    <errorConditionFormula>OR(  Payroll_Receive_Info_Post__c &gt; 1.0,
Payroll_Receive_Info_Post__c  &lt; 0.0
)</errorConditionFormula>
